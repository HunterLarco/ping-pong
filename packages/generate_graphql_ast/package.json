    "graphql": "16.6.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "9.0.2",
    "@trivago/prettier-plugin-sort-imports": "4.0.0",
    "prettier": "2.7.1",
    "rollup": "3.3.0",
    "tslib": "2.4.1",
    "typescript": "4.9.3"
  }
